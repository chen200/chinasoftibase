	<sql id="Base_Column_List">
		IDS, URLTOKEN, APPID, OPENID, 
		SUBSCRIBE, SUBSCRIBE_TIME, NICKNAME, 
		SEX, COUNTRY, PROVINCE, CITY, LANGUAGE, HEADIMGURL, 
		GROUPID, UNIONID, TAGID_LIST, BLACK, REMARK 
