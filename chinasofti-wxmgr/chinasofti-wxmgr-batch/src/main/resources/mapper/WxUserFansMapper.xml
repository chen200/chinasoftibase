		id="BaseResultMap">
		<id column="IDS" property="ids" jdbcType="VARCHAR" />
		<result column="URLTOKEN" property="urltoken" jdbcType="VARCHAR" />
		<result column="APPID" property="appid" jdbcType="VARCHAR" />
		<result column="OPENID" property="openid" jdbcType="VARCHAR" />
		<result column="SUBSCRIBE" property="subscribe" jdbcType="CHAR" />
		<result column="SUBSCRIBE_TIME" property="subscribeTime"
			jdbcType="VARCHAR" />
		<result column="NICKNAME" property="nickname" jdbcType="VARCHAR" />
		<result column="SEX" property="sex" jdbcType="CHAR" />
		<result column="COUNTRY" property="country" jdbcType="VARCHAR" />
		<result column="PROVINCE" property="province" jdbcType="VARCHAR" />
